<launch>

  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="multi_robot_name" default=""/>
  <arg name="x_pos" default="-0.2"/>
  <arg name="y_pos" default="1.6"/>
  <arg name="z_pos" default="0"/>  
  <arg name="yaw_pos" default="3.14"/>  

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
   <arg name="world_name" value="$(find aue_finals)/worlds/turtlebot_final.world" />
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>  

   <!--<include file="$(find person_sim)/launch/init_small_standing_person.launch">
   </include>-->

   <!--<include file="$(find turtle_tf_3d)/launch/person_standing_keyboard_move.launch">
   </include>-->

  <!--<include file="$(find turtlebot3_bringup)/launch/includes/description.launch.xml">
    <arg name="model" value="$(arg model)" />
  </include>-->

  <!--<node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher">
    <param name="publish_frequency" type="double" value="50.0" />
    <param name="tf_prefix" value="$(arg multi_robot_name)"/>
  </node>-->

   <group ns="RobotA" clear_params="true">
    <param name="robot_description" command="$(find xacro)/xacro --inorder $(find turtlebot3_description)/urdf/turtlebot3_burger.urdf.xacro" />
    <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model turtlebot3_$(arg model) -x $(arg x_pos) -y $(arg y_pos) -z $(arg z_pos) -Y $(arg yaw_pos) -param robot_description" />
      <remap from="scan" to="/scan" />
  </group> 

  <group ns="RobotB" clear_params="true">
    <arg name="B_x_pos" default="1.5"/>
    <arg name="B_y_pos" default="0.9"/>
    <arg name="B_z_pos" default="0"/>  
    <arg name="B_yaw_pos" default="1.57"/>  
    <param name="robot_description" command="$(find xacro)/xacro --inorder $(find aue_finals)/urdf/turtlebot3_apriltag.urdf.xacro" />
    <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model turtlebot3_apriltag -x $(arg B_x_pos) -y $(arg B_y_pos) -z $(arg B_z_pos) -Y $(arg B_yaw_pos) -param robot_description" respawn="false"/>
  </group>
  
  <group ns="RobotC" clear_params="true">
    <arg name="C_x_pos" default="1.5"/>
    <arg name="C_y_pos" default="0.0"/>
    <arg name="C_z_pos" default="0"/>  
    <arg name="C_yaw_pos" default="1.57"/>  
    <param name="robot_description" command="$(find xacro)/xacro --inorder $(find aue_finals)/urdf/turtlebot3_rpi_camera.urdf.xacro" />
    <node pkg="gazebo_ros" type="spawn_model" name="spawn_urdf" args="-urdf -model turtlebot3 -x $(arg C_x_pos) -y $(arg C_y_pos) -z $(arg C_z_pos) -Y $(arg C_yaw_pos) -param robot_description" respawn="false"/>
  </group>

  <arg name="launch_prefix" default="" /> <!-- set to value="gdbserver localhost:10000" for remote debugging -->
  <arg name="node_namespace" default="apriltag_ros_continuous_node" />
  <arg name="camera_name" default="RobotC/camera/rgb" />
  <arg name="camera_frame" default="RobotC/camera_rgb_optical_frame" />
  <arg name="image_topic" default="image_raw" />

  <!-- Set parameters -->
  <rosparam command="load" file="$(find turtlebot3_mr)/config/settings.yaml" ns="$(arg node_namespace)" />
  <rosparam command="load" file="$(find turtlebot3_mr)/config/tags.yaml" ns="$(arg node_namespace)" />
  
  <node pkg="apriltag_ros" type="apriltag_ros_continuous_node" name="$(arg node_namespace)" clear_params="true" output="screen" launch-prefix="$(arg launch_prefix)" >
    <!-- Remap topics from those used in code to those on the ROS network -->
    <remap from="image_rect" to="$(arg camera_name)/$(arg image_topic)" />
    <remap from="camera_info" to="$(arg camera_name)/camera_info" />

    <param name="camera_frame" type="str" value="$(arg camera_frame)" />
    <param name="publish_tag_detections_image" type="bool" value="true" />      <!-- default: false -->
  </node>

  <!-- <group ns="tagA" clear_params="true">
    <node pkg="gazebo_ros" type="spawn_model" name="spawn_apriltag" args="-sdf -file $(find aue_finals)/models/Apriltag36_11_00000/model.sdf -model tag_0 -x -1.7 -y 0 -z 0.2 -P -1.57 -Y 1.57" />
  </group>
  <group ns="tagB" clear_params="true">
  <node pkg="gazebo_ros" type="spawn_model" name="spawn_apriltag" args="-sdf -file $(find aue_finals)/models/Apriltag36_11_00000/model.sdf -model tag_1 -x 0 -y -1.7 -z 0.2 -P -1.57" />
  </group> -->
  <!-- <group ns="tagC" clear_params="true">
    <node pkg="gazebo_ros" type="spawn_model" name="spawn_apriltag" args="-sdf -file $(find aue_finals)/models/Apriltag36_11_00000/model.sdf -model apriltag_marker -x 1.7 -y 0 -z 0.2 -P -1.57 -Y 1.57" />
  </group> -->
  <!--<node name="rviz" pkg="rviz" type="rviz" args="-d $(find turtlebot3_gazebo)/rviz/turtlebot3_gazebo_model.rviz"/>-->
</launch>




